<?xml version='1.0' encoding='ISO-8859-1' ?>
<!-- javacc.xml. Defines JavaCC properties, imported by build.xml.
     Francis Andre - 01/2017 -->
<project name="javacc_props" default="echo_javacc_props">

	<!-- Set this JavaCC jar's directory according to your environment -->
	<property name="javacc_imp_dir" value="D:\Devs\Parsers-workspace\javacc-svn_java.net\target" />
	<!--property name="javacc_imp_dir" value="Z:/git/javacc/target" / -->

	<!-- JavaCC jar to use -->
	<property name="javacc_ok_jar" value="${javacc_imp_dir}/javacc.jar" />
	<property name="javacc_main" value="org.javacc.parser.Main" />

	<!-- echo some JavaCC / JJTree properties -->
	<target name="echo_javacc_props">
		<echo message="javacc_imp_dir=${javacc_imp_dir}" />
		<echo message="javacc_ok_jar=${javacc_ok_jar}" />
		<echo message="javacc_main=${javacc_main}" />
	</target>
</project>